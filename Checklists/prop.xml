<?xml version="1.0"?>
<!-- Throttle check -->
<PropertyList>
		<condition>
			<and>
				<greater-than-equals>
					<property>/controls/engines/engine/propeller-pitch</property>
					<value alias="../../../../min-prop"/>
				</greater-than-equals>
				<less-than-equals>
					<property>/controls/engines/engine/propeller-pitch</property>
					<value alias="../../../../max-prop"/>
				</less-than-equals>
			</and>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/engines/engine/propeller-pitch</property>
			<value alias="../../min-prop"/>
		</binding>
		<marker>
			<x-m>2.7725</x-m>
			<y-m>0.2383</y-m>
			<z-m>0.6433</z-m>
			<scale>2</scale>
		</marker>
</PropertyList>
