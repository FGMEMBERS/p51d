<?xml version="1.0"?>
<PropertyList>

 <path>wood-seat.ac</path>

    <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>30</max-m>
    </animation>

 <animation>
  <type>material</type>
  <object-name>SeatBottom</object-name>
  <object-name>SeatBack</object-name>
  <object-name>HeadRest</object-name>
  <emission>
    <factor-prop>controls/lighting/cabin-norm</factor-prop>
    <red>0.28</red>
    <green>0.18</green>
    <blue>0.18</blue>
  </emission>
 </animation>

</PropertyList>