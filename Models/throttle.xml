 <animation>
  <type>rotate</type>
  <object-name>ThrottleGrip</object-name>
  <object-name>micButton</object-name>
  <object-name>ThrottleLever</object-name>
  <object-name>ThrottleGripStop</object-name>
  <object-name>rangeLever</object-name>
  <object-name>rangeCableHousing</object-name>
  <object-name>RangeCableBracket</object-name>
  <property>/controls/engines/engine[0]/throttle</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>100</dep>
   </entry>
  </interpolation>  
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation> 
 
