 <animation>
  <type>rotate</type>
  <object-name>PropHandle</object-name>
  <object-name>PropKnob</object-name>aa
  <property>/controls/engines/engine[0]/propeller-pitch</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>-100</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>0</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>MixtureHandle</object-name>
  <object-name>MixtureKnob</object-name>
  <property>/controls/engines/engine[0]/mixture</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>60</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>translate</type>
  <object-name>micButton</object-name>
  <property>instrumentation/comm/SCR-522C/remote-pushed</property>
  <factor>-0.007</factor>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

