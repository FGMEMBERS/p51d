<?xml version="1.0"?>
<PropertyList>

 <path>p51d-jw-engine.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>NeedleOilTemp</object-name>
  <property>engines/engine[0]/oil-temperature-degf</property>
  <min>32</min>
  <max>212</max>
  <interpolation>
   <entry>
    <ind>32</ind>
    <dep>-30</dep>
   </entry>
   <entry>
    <ind>212</ind>
    <dep>123</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NeedleOilPress</object-name>
  <property>engines/engine[0]/oil-pressure-psi</property>
  <min>0</min>
  <max>200</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>200</ind>
    <dep>180</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>-0.0087</y-m>
   <z-m>-0.0116</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NeedleFuel</object-name>
  <property>engines/engine/fuel-pump-psi</property>
  <min>0</min>
  <max>25</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>25</ind>
    <dep>180</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0.0087</y-m>
   <z-m>-0.0116</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>NeedleOilTemp</object-name>
  <object-name>NeedleOilPress</object-name>
  <object-name>NeedleFuel</object-name>
  <emission>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
    <red>0.35</red>
    <green>0.35</green>
    <blue>0.4</blue>
  </emission>
 </animation>

</PropertyList>
